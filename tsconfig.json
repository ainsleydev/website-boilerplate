{
	"compilerOptions": {
		"target": "ES2015",
		"module": "commonjs",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"noImplicitAny": false,
		"noImplicitReturns": false,
		"strictPropertyInitialization": false,
		"lib": [
			"es2016",
			"dom",
			"dom.iterable"
		]
	},
	"types": [
		"node"
	],
	"typeRoots": [
		"./node_modules/@types"
	],
	"paths": {
		"*": ["types/*.d.ts"]
	},
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "Recommended"
}
